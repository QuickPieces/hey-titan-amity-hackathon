{
  "manifest_version": 3,
  "permissions": ["tabs"],
  "background": {
    "service_worker": "index.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "name": "My Chrome Extension",
  "version": "1.0",
  "description": "This is my first Chrome extension",
  "icons": {
    "16": "./icons/icon.png",
    "48": "./icons/icon.png",
    "128": "./icons/icon.png"
  }
}
